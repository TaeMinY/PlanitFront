ity","sourcesContent":["<template>\n  <div class=\"see\">\n    <div class=\"see__title\">\n      <div>Community</div>\n      <img\n        @click=\"create()\"\n        alt=\"create\"\n        src=\"../../assets/playlist_add-24px.svg\"\n        width=\"42px\"\n        style=\"cursor: pointer;\"\n      />\n    </div>\n\n    <div v-for=\"(value,index) in data\" :key=\"index\">\n      <div class=\"see__box\">\n        <div style=\"display:flex;align-items:center;\">\n          <div class=\"see__left\">\n            <div class=\"see__article__profile\">\n              <div style=\"display:flex; align-items:center\">\n                <img\n                  class=\"see__article__image\"\n                  :src=\"'http://taemin.dev/planit/'+value.email+'.png'\"\n                  alt=\"profile\"\n                  width=\"42px\"\n                  height=\"42px\"\n                  style=\"object-fit:cover\"\n                />\n                <div class=\"see__article__name\">{{value.name}}</div>\n              </div>\n              <div\n                class=\"see__article__unlike\"\n                @click=\"like(value)\"\n                v-if=\"checkLike(value)\"\n              >응원중 {{value.like}}</div>\n              <div class=\"see__article__like\" @click=\"like(value)\" v-else>응원해요 {{value.like}}</div>\n            </div>\n\n            <div class=\"see__article__title\">{{value.data.title}}</div>\n            <div class=\"see__article__subtitle\">{{value.data.text}}</div>\n\n            <div class=\"see__article__date\">{{value.data.startDay}} ~ {{value.data.endDay}}</div>\n\n            <div class=\"see__article__text\">{{value.text}}</div>\n\n            <div class=\"see__article__comment\">\n              <input\n                type=\"text\"\n                placeholder=\"댓글 달기\"\n                class=\"see__article__comment__input\"\n                v-on:keyup.enter=\"submit(value._id,index)\"\n                :id=\"value._id\"\n              />\n              <input\n                type=\"submit\"\n                value=\"등록\"\n                class=\"see__article__comment__submit\"\n                @click=\"submit(value._id,index)\"\n              />\n            </div>\n          </div>\n\n          <div class=\"see__right\">\n            <img\n              class=\"see__article__undraw\"\n              :src=\"require(`@/assets/article_images/undraw_article_${(index%4+1)}.svg`)\"\n              alt=\"article image\"\n            />\n          </div>\n        </div>\n\n        <div class=\"see__left\">\n          <div v-if=\"statusif(value._id)\">\n            <div\n              class=\"see__article__comment__enable\"\n              @click=\"status(index)\"\n              v-if=\"(value.status != true)\"\n            >▼ Show Comments</div>\n            <div class=\"see__article__comment__enable\" @click=\"status(index)\" v-else>▲ Hide Comments</div>\n          </div>\n          <div v-if=\"(value.status == true)\">\n            <div v-for=\"(d,index) in commentData\" :key=\"index\" v-if=\"(d.post_id == value._id)\">\n              <div class=\"see__comment\">\n                <div style=\"display:flex;align-items:center;\">\n                  <img\n                    class=\"see__comment__image\"\n                    :src=\"'http://taemin.dev/planit/'+d.email+'.png'\"\n                    alt=\"comment profile\"\n                  />\n                  <div class=\"see__comment__name\">{{d.name}}</div>\n                  <div class=\"see__comment__text\">{{d.text}}</div>\n                </div>\n                <div style=\"display:flex;align-items:center;\">\n                  <div class=\"see__comment__time\">{{d.time}}</div>\n                  <div\n                    class=\"see__comment__delete\"\n                    @click=\"deletecomment(d)\"\n                    v-if=\"d.email == userdata.email\"\n                  >×</div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"community\",\n  data() {\n    return {\n      data: [],\n      commentData: [],\n      commentStatus: [],\n      userdata: {},\n      enter: false\n    };\n  },\n  computed: {},\n  components: {},\n  created() {\n    this.$store\n      .dispatch(\"token\", {\n        token: localStorage.getItem(\"token\")\n      })\n      .then(response => {\n        if (response.data.result == true) {\n          this.userdata = response.data.userdata;\n        } else {\n        }\n      })\n      .catch(e => {});\n    this.$store\n      .dispatch(\"FIND__POST\", {\n        token: localStorage.getItem(\"token\")\n      })\n      .then(response => {\n        if (response.data.result == true) {\n          var fal = { status: true };\n          this.data = response.data.userdata.map(v => Object.assign(v, fal));\n        } else {\n        }\n      })\n      .catch(e => {});\n    this.$store\n      .dispatch(\"FIND__COMMENT__ALL\", {})\n      .then(response => {\n        if (response.data.result == true) {\n          this.commentData = response.data.userdata;\n        } else {\n        }\n      })\n      .catch(e => {});\n  },\n  methods: {\n    deletecomment(e) {\n      this.$store\n        .dispatch(\"DELETE__COMMENT\", {\n          token: localStorage.getItem(\"token\"),\n          _id: e._id\n        })\n        .then(response => {\n          this.$store\n            .dispatch(\"FIND__COMMENT__ALL\", {})\n            .then(response => {\n              if (response.data.result == true) {\n                this.commentData = response.data.userdata;\n              } else {\n              }\n            })\n            .catch(e => {});\n        });\n    },\n    checkLike(e) {\n      for (let i = 0; i < e.like_users.length; i++) {\n        if (e.like_users[i] == this.userdata.email) {\n          return true;\n        }\n      }\n      return false;\n    },\n    like(e) {\n      this.$store\n        .dispatch(\"POST__LIKE\", {\n          token: localStorage.getItem(\"token\"),\n          _id: e._id\n        })\n        .then(response => {\n          this.$store\n            .dispatch(\"FIND__POST\", {\n              token: localStorage.getItem(\"token\")\n            })\n            .then(response => {\n              if (response.data.result == true) {\n                var fal = { status: true };\n                this.data = response.data.userdata.map(v =>\n                  Object.assign(v, fal)\n                );\n              } else {\n              }\n            })\n            .catch(e => {});\n        });\n    },\n    statusif(e) {\n      var Sa = false;\n      for (let i = 0; i < this.commentData.length; i++) {\n        if (this.commentData[i].post_id == e) {\n          Sa = true;\n        }\n      }\n      if (Sa == true) {\n        console.log(\"true\");\n        return true;\n      } else {\n        console.log(\"false\");\n        return false;\n      }\n    },\n    status(e) {\n      console.log(e);\n      this.data[e].status = !this.data[e].status;\n    },\n    create() {\n      this.$router.push(\"/wrap/community/create\");\n    },\n    submit(e, index) {\n      if (this.enter == false) {\n        this.enter = true;\n        var d = new Date();\n        var nowDate =\n          d.getFullYear() +\n          \"-\" +\n          (d.getMonth() + 1) +\n          \"-\" +\n          d.getDate() +\n          \"-\" +\n          d.getHours() +\n          \"-\" +\n          d.getMinutes() +\n          \"-\" +\n          d.getSeconds();\n        this.$store\n          .dispatch(\"CREATE__COMMENT\", {\n            token: localStorage.getItem(\"token\"),\n            _id: e,\n            text: document.getElementById(e).value,\n            time: nowDate\n          })\n          .then(response => {\n            if (response.data.result == true) {\n              console.log(\"요청확인\");\n              this.$store\n                .dispatch(\"FIND__COMMENT__ALL\", {})\n                .then(response => {\n                  if (response.data.result == true) {\n                    this.commentData = response.data.userdata;\n                    document.getElementById(e).value = \"\";\n                    this.data[index].status = true;\n                    this.enter = false;\n                  } else {\n                  }\n                })\n                .catch(e => {\n                  console.log(e);\n                });\n            } else {\n            }\n          })\n          .catch(e => {\n            console.log(e);\n          });\n      }\n    }\n  }\n};\n</script>\n<style scoped>\n.see {\n  padding: 1.5% 3.5%;\n  width: 100%;\n  height: 100%;\n  flex: 1;\n}\n.see__title {\n  font-size: 40px;\n  min-height: 60px;\n  font-style: normal;\n  font-family: ProductSansM;\n  color: #000000;\n  text-align: left;\n  margin-left: 10px;\n  margin-top: 10px;\n  margin-right: 0;\n  margin-bottom: 0;\n  padding: 0;\n  display: flex;\n  justify-content: space-between;\n  overflow-y: hidden;\n}\n.see__box {\n  display: flex;\n  flex-direction: column;\n  width: 100%;\n  min-height: 200px;\n  margin-top: 28px;\n  margin-bottom: 24px;\n  background-color: white;\n  border-radius: 30px;\n  box-sizing: border-box;\n  padding: 50px;\n  position: relative;\n}\n.see__left {\n  width: 65%;\n}\n.see__right {\n  display: flex;\n  align-items: top;\n  justify-content: center;\n  width: 35%;\n}\n.see__article__profile {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n.see__article__image {\n  border-radius: 100%;\n  background-color: #eaf3fe;\n  cursor: pointer;\n  width: 42px;\n}\n.see__article__name {\n  margin-left: 8px;\n  font-family: \"ProductSansR\", \"NanumSB\";\n  font-size: 18px;\n}\n.see__article__title {\n  word-break: break-all;\n  color: black;\n  margin-top: 16px;\n  text-align: left;\n  font-family: \"ProductSansM\", \"NanumSB\";\n  font-size: 28px;\n}\n.see__article__subti